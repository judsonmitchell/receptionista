<h1>Welcome to {{ org_name }}</h1>
<h3>My name is {{ name }} </h3>
<hr />
<div class="row">
    <div class="col-lg-6">
        <video id="self" width="100%" height="100%" muted="muted"></video>
    </div>
    <div class="col-lg-6">
        <video id="caller" width="100%" height="100%"></video>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <button class="btn btn-large btn-danger video-start">Start</button>
        <button class="btn btn-large btn-danger video-stop">Stop</button>
    </div>
</div>
